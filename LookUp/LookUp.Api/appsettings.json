{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Log/LookUp.Api.Log_.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "DefaultCandidate": {
      "name": "test",
      "phone": "test"
  },
  "AllowedHosts": "*"
}
